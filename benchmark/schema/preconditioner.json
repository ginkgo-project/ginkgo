{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "Preconditioner",
  "desciption": "something",
    "type": "object",
    "allOf": [
      {
        "$ref": "spmv.reuse.json"
      },
      {
        "properties": {
          "operator": {
            "$ref": "operator.json"
          },
          "preconditioner": {
            "type": "object"
          }
        }
      }
    ],
  "examples": [
    {
      "operator": {
        "filename": "my_file.mtx"
      },
      "preconditioner": {
        "type": "preconditioner::Jacobi"
      },
      "format": "csr",
      "reorder": "nd"
    },
    {
      "operator": {
        "stencil": {
          "name": "5pt",
          "size": 100
        }
      },
      "preconditioner": {
        "type": "preconditioner::GaussSeidel",
        "symmetric": true
      }
    }
  ]
}
